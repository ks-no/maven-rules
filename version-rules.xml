<?xml version="1.0" encoding="UTF-8"?>
<ruleset xmlns="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" comparisonMethod="maven" xsi:schemaLocation="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0 http://mojo.codehaus.org/versions-maven-plugin/xsd/rule-2.0.0.xsd">
   <ignoreVersions>
      <ignoreVersion type="regex">.*[-_\.](alpha|Alpha|ALPHA)[-_\.]?[0-9]*</ignoreVersion>
      <ignoreVersion type="regex">.*[-_\.](b|beta|Beta|BETA)[-_\.]?[0-9]*(.[0-9]*)?</ignoreVersion>
      <ignoreVersion type="regex">.*[-_\.](rc|RC)[-_\.]?[0-9]*</ignoreVersion>
      <ignoreVersion type="regex">.*[-_\.]M[-_\.]?[0-9]*</ignoreVersion>
      <ignoreVersion type="regex">.*[-_\.]EA[-_\.]?[0-9]*</ignoreVersion>
      <ignoreVersion type="regex">.*[-_\.]jre[-_\.]?[0-9]*</ignoreVersion>
      <ignoreVersion type="regex">.*[-_\.]preview[-_\.]?[0-9]*</ignoreVersion>
      <ignoreVersion type="regex">.*[-_\.]jbossorg[-_\.]?[0-9]*</ignoreVersion>
   </ignoreVersions>
</ruleset>
